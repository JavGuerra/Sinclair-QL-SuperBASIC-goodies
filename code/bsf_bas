100 REMark by Badaman
110 DEFine FuNction BSF
120   LOCal r, d, c, n, i
130   LET r = 84 : REMark Basic QL Factor
140   LET d = DATE : c = 0 : n = 0
150   REPeat i
160     IF DATE <> d THEN 
170       d = DATE
180       n = n + 1
190       IF n = 1 THEN 
200         c = 0
210       ELSE 
220         EXIT i
230       END IF 
240     ELSE 
250       c = c + 1
260     END IF 
270   END REPeat i
280   REMark PRINT c : REMark count
290   RETurn c / r
300 END DEFine BSF